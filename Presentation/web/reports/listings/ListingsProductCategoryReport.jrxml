	<queryString>
		<![CDATA[select distinct
    com_users.user_id,
    com_users.user_firstname,
    com_users.user_lastname,
    listing_price,
    listing_product_name,
    listing_code,
    listing_creation_date,
    trg_product_categories.product_category_id,
    trg_product_categories.product_category_name,
    trg_product_statuses.product_status_id,
    trg_product_statuses.product_status_name,
    trg_transaction_types.transaction_type_id,
    trg_transaction_types.transaction_type_name
from
    com_users,
    trg_listings,
    trg_product_categories,
    trg_product_statuses,
    trg_transaction_types
where trg_listings.user_id = com_users.user_id
and   trg_listings.product_status_id =  trg_product_statuses.product_status_id
and   trg_listings.transaction_type_id =  trg_transaction_types.transaction_type_id
and   trg_listings.product_category_id =  trg_product_categories.product_category_id
and   trg_product_categories.product_category_id = $P{PRODUCT_CATEGORY_ID}
order by listing_creation_date]]>
	</queryString>
